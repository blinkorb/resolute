{
  "name": "@blinkorb/resolute-example",
  "version": "0.0.0",
  "description": "Resolute example project",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build-ssg": "(cd ../resolute ; npm run build) && resolute build",
    "dev-ssg": "(cd ../resolute ; npm run build) && resolute dev",
    "typecheck": "tsc --noEmit",
    "format-check": "prettier --check '**/*'",
    "format": "prettier --write '**/*'",
    "lint": "eslint './src/**/*.{ts,tsx,js,jsx}'",
    "tests": "jest --coverage --runInBand",
    "test": "npm run typecheck && npm run format-check && npm run lint && npm run tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blinkorb/resolute.git"
  },
  "keywords": [
    "resolute",
    "example",
    "react",
    "static",
    "site"
  ],
  "author": "BlinkOrb",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/blinkorb/resolute/issues"
  },
  "homepage": "https://github.com/blinkorb/resolute#readme",
  "dependencies": {
    "@blinkorb/resolute": "^0.0.0",
    "@types/node": "^18.17.4",
    "@types/react": "^18.2.18",
    "@types/react-dom": "^18.2.7",
    "@types/react-helmet": "^6.1.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-jss": "^10.10.0",
    "typescript": "^5.2.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.4",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1"
  }
}
